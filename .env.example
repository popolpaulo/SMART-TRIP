# Configuration de l'environnement de développement
# Copiez ce fichier en .env et adaptez les valeurs

# Serveur
NODE_ENV=development
PORT=3000
HOST=localhost

# Base de données PostgreSQL
DB_HOST=localhost
DB_PORT=5433
DB_NAME=smarttrip_dev
DB_USER=smarttrip_user
DB_PASSWORD=smarttrip_password

# Sécurité
JWT_SECRET=votre_secret_jwt_tres_complexe_a_changer
JWT_EXPIRE=7d

# API externes (à compléter plus tard)
FLIGHT_API_KEY=
HOTEL_API_KEY=
VPN_API_KEY=
WEATHER_API_KEY=
OPENAI_API_KEY=

# CORS
CORS_ORIGIN=http://localhost:5173

# Logs
LOG_LEVEL=debug
